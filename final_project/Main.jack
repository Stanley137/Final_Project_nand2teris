class Main {
    function void main() {
        var Game game;
        var int result;
        // level 1
        let game = Game.new(); 
        do game.init(1, 5, 4, 1); // (level, jet_hp, num_enemy, enemy_hp)
        let result = game.run();
        if(result = -1){
            do game.over();
        }
        return;
    }
}